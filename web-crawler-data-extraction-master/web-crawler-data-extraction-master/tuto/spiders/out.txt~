  //<![CDATA[
    $(document).ready(function(){
      if ( "true" === "true"){
        var $posterAndTracks = $(".cassette .track > a, .poster");
        var rightRail = BLOOMBERG.BVP.rightRail;
    
        if ( "false" === "true"){
          $posterAndTracks.click( function(e) {
            e.preventDefault();
            rightRail.loadBVP("us", "tv");
          });
    
        }
        else {
          $(".cassette .track > a").on('click touchstart', function(e) {
            var ogEvent = e.originalEvent;
            if(ogEvent.type === 'touchstart' && ogEvent.touches.length > 1) {
              return;
            }
    
            e.preventDefault();
    
            $this = $(this);
            var _video_id = $this.attr("data-video-id");
            rightRail.markCurrentTrack($this.parent());
            rightRail.loadBVP( _video_id, 'video', '.poster', '301', '175', "blp.video/vod2/right-rail/technology" );
          });
    
          $(".poster").click( function(e) {
            e.preventDefault();
    
            $firstTrack = $(".cassette .track a").first();
            var _video_id = $firstTrack.attr("data-video-id");
            rightRail.markCurrentTrack( $firstTrack.parent() );
            rightRail.loadBVP( _video_id, 'video', '.poster', '301', '175', "blp.video/vod2/right-rail/technology" );
          });
        }
    
      }
    });
  //]]>
</script>

